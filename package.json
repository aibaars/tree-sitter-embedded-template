{
  "name": "tree-sitter-embedded-template",
  "version": "0.13.0",
  "description": "Embedded template grammar for tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "json"
  ],
  "author": "Max Brunsfeld",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.0.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.14.5"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "text.html.ejs",
      "file-types": ["ejs"],
      "highlights": "src/highlights-ejs.json",
      "injection-regex": "ejs"
    },
    {
      "scope": "text.html.erb",
      "file-types": ["erb"],
      "highlights": "src/highlights-erb.json",
      "injection-regex": "erb"
    }
  ]
}
